prefix=@PROJECT_NAME@
exec_prefix=${prefix}
libdir=${prefix}/@CMAKE_INSTALL_LIBDIR@
includedir=${prefix}/@CMAKE_INSTALL_INCLUDEDIR@/@PROJECT_NAME@

Name: @PROJECT_NAME@
Description: @CMAKE_PROJECT_DESCRIPTION@
Version: @CMAKE_PROJECT_VERSION@

Libs: -L${libdir} -l@PROJECT_NAME@
Cflags: -I${includedir}